{
  "name": "til_web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "local": "next dev",
    "prod": "env-cmd -f .env.production next dev",
    "build": "next build",
    "build:dev": "env-cmd -f .env.development next build",
    "start": "next start",
    "start:dev": "env-cmd -f .env.development next start",
    "pm2:reload:dev": "env-cmd -f .env.development pm2 reload ecosystem.config.js --only til-dev --env development",
    "pm2:reload:prod": "env-cmd -f .env.production pm2 reload ecosystem.config.js --only til-product --env production",
    "pm2:start:dev": "env-cmd -f .env.development pm2 start ecosystem.config.js --only til-dev --env development",
    "pm2:start:prod": "env-cmd -f .env.production pm2 start ecosystem.config.js --only til-product --env production && pm2 monit",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@tanstack/react-query": "^4.22.0",
    "axios": "^1.2.2",
    "cookies-next": "^2.1.1",
    "date-fns": "^2.29.3",
    "env-cmd": "^10.1.0",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-datepicker": "^4.10.0",
    "react-dom": "18.2.0",
    "react-use": "^17.4.0",
    "recoil": "^0.7.6",
    "swiper": "^9.0.5"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.10.5",
    "@svgr/webpack": "^6.5.1",
    "@tanstack/react-query-devtools": "^4.22.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "18.0.10",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.1",
    "prettier": "^2.8.2",
    "typescript": "4.9.4"
  }
}
